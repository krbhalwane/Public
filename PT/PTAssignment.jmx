            <stringProp name="cacheKey">false</stringProp>
            <stringProp name="script">// get variables from JSON extractor 
def CategoryId = &quot;Category: &quot; + vars.get(&quot;cCategoryId&quot;);
def Name = &quot;Name: &quot; + vars.get(&quot;cName&quot;);
def Path = &quot;Path: &quot; + vars.get(&quot;cPath&quot;);
def Promotions = &quot;Promotions: &quot; + vars.get(&quot;cPromotions_ALL&quot;);


// pass &quot;true&quot; if want to append to existing file  
// if want to overwrite, then don&apos;t pass the second argument &quot;true&quot;. 
 FileWriter fstream = new FileWriter(&quot;C://Users//DELL//Desktop//NZ//PT//ExportData.csv&quot;, true);
 BufferedWriter out = new BufferedWriter(fstream);

 out.write(CategoryId+&quot;,&quot;+Name+&quot;,&quot;+Path+&quot;,&quot;+Promotions);

 out.write(System.getProperty(&quot;line.separator&quot;));
 out.close();
 fstream.close();

 </stringProp>
